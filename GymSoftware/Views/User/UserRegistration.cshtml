@{
    ViewBag.Title = "UserRegistration";
    Layout = "~/Views/Shared/_NewLayout.cshtml";


}
<div class="row">
    <div class="col-lg-12">
        <div class="card ">
            <div class="card-header bg-info">
                <h4 class="m-b-0 text-white">Customer Registration</h4>
            </div>
            <div class="card-body">
                    <div class="form-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Name</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter Name">
                                        @*<small class="form-control-feedback"> This is inline help </small>*@
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Gender</label>
                                    <div class="col-md-9">
                                        <div class="custom-control custom-radio">
                                            <input type="radio" id="customRadio3" name="member" class="custom-control-input">
                                            <label class="custom-control-label" for="customRadio3">Male</label>
                                        </div>
                                        <div class="custom-control custom-radio">
                                            <input type="radio" id="customRadio3" name="member" class="custom-control-input">
                                            <label class="custom-control-label" for="customRadio3">Female</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <div class="row">
                            
                            <!--/span-->
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Date of Birth</label>
                                    <div class="col-md-9">
                                        <input type="date" class="form-control" placeholder="dd/mm/yyyy">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Source</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter Source">
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Weight</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter weight">
                                        @*<small class="form-control-feedback"> This is inline help </small>*@
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Memebership</label>
                                    <div class="col-md-9">
                                        <select class="form-control custom-select" id="gen">
                                            <option value="">-- Select--</option>
                                            <option value="">1 Month</option>
                                            <option value="">3 Months</option>
                                            <option value="">6 Months</option>
                                            <option value="">1 Year</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <!--/span-->
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Batch Tymings</label>
                                    <div class="col-md-9">
                                        <select class="form-control custom-select" id="gen">
                                            <option value="">-- Select--</option>
                                            <option value="">Morning (6 Am - 10 AM)</option>
                                            <option value="">Evening (7 PM - 11 PM)</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Total Payment</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter Total Payment">
                                        @*<small class="form-control-feedback"> This is inline help </small>*@
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <!--/span-->
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Payment</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter Payment">
                                        @*<small class="form-control-feedback"> This is inline help </small>*@
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="control-label text-right col-md-3">Outstanding Amount</label>
                                    <div class="col-md-9">
                                        <input type="text" id="txtfname" class="form-control" placeholder="Enter Outstanding">
                                        @*<small class="form-control-feedback"> This is inline help </small>*@
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <!--/span-->
                        </div>

                        <div class="row">
                            <div class="container">
                                <center>
                                    <button type="submit" id="btnGetData" class="btn btn-primary">Register</button>
                                </center>
                            </div>
                        </div>
                        
                        <!--/span-->
                    </div>
                   
                    
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script>
    $(document).ready(function () {
        //var apiBaseUrl = "http://localhost:54874//";
        $('#btnGetData').click(function () {

            var Registration = new Object();
            Registration.FirstName = $('#txtfname').val();
            Registration.LastName = $('#txtlastname').val();
            Registration.Gender = $('#gen').val();


            $.ajax({
                url: 'http://localhost:2154/api/User/UserRegistration/',
                type: 'POST',
                data: JSON.stringify(Registration),
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (data) {
                    console.log(data);
                },
                error: function () {
                    alert('Error!');
                }
            });
        });
    });
</script>


